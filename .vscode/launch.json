      "args": ["--seturl", "https://www.meshtastic.org/d/#CgIYAw"
      ]
    },
    {
      "name": "meshtastic shell",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--debug", "--seriallog", "stdout"]
    },
    {
      "name": "meshtastic test",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--debug", "--test"]
