  "version": "0.2.0",
  "configurations": [
    {
      "name": "meshtastic BLE",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": false,
      "args": ["--debug", "--ble", "--device", "24:62:AB:DD:DF:3A"]
    },
    {
      "name": "meshtastic admin",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--debug", "--setch-longslow", "--port", "/dev/ttyUSB1"]
    },
    {
      "name": "meshtastic tunnel",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--tunnel", "--debug"]
    },
    {
      "name": "meshtastic set chan",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--setchan", "psk", "0x1a1a1a1a2b2b2b2b1a1a1a1a2b2b2b2b1a1a1a1a2b2b2b2b1a1a1a1a2b2b2b2b", "--debug"]
    },
    {
      "name": "meshtastic debug",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--debug" ]
    },
    {
      "name": "meshtastic setpref",
      "type": "python",
      "request": "launch",
      "module": "meshtastic",
      "justMyCode": true,
      "args": ["--debug", "--setchan", "psk", ""]
